{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["next", "google", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "require-jsdoc": 0,
    "react/react-in-jsx-scope": 0,
    "react/prop-types": 0,

    // "import/no-anonymous-default-export": 0,
    // "react-hooks/exhaustive-deps": 0,
    "react-hooks/rules-of-hooks": 0
    // "@next/next/no-img-element":0
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
