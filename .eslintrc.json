{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["next/core-web-vitals", "google", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "require-jsdoc": 0,
    "jsx-a11y/alt-text": [
      1,
      {
        "elements": ["img"]
      }
    ]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
