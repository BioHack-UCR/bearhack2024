{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["next/core-web-vitals", "google", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react","jsx-a11y"],
  "rules": {
    "require-jsdoc": 0,
    "jsx-a11y/alt-text": ["error", {
      "elements": ["img"]
    }]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
